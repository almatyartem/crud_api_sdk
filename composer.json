{
    "name": "turbodev/games_api_sdk",
    "description": "GamesAPI SDK",
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:almatyartem/guzzle_wrapper.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:almatyartem/response_wrapper.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:almatyartem/logging_request_provider.git"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "turbodev/guzzle_wrapper": "dev-master",
        "turbodev/logging_request_provider": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "GamesApiSdk\\": "src/"
        }
    }
}
